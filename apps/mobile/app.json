{
  "expo": {
    "name": "Gestimate",
    "slug": "gestimate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "gestimate",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#c084fc"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gestimate.app"
    },
    "android": {
      "package": "com.gestimate.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#c084fc"
      }
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "7fbd11ef-d6e5-45f3-a5c6-e8a907a0bcc3"
      }
    }
  }
}
